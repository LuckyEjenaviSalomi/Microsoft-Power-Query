// return a table with empty rows removed
(tbl as table)=>
let
    replace_empties = Table.ReplaceValue(tbl," ", null, Replacer.ReplaceValue, Table.ColumnNames(tbl)),
    table_profile = Table.Profile(replace_empties),
    RemovedOtherColumns = Table.SelectColumns(table_profile,{"Column", "NullCount"})
    in Table.SelectRows(RemovedOtherColumns, each ([NullCount] = Table.RowCount(tbl)))[Column]
